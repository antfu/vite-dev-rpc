{
  "name": "vite-dev-rpc",
  "version": "0.0.0",
  "packageManager": "pnpm@6.32.3",
  "description": "Remote procedure call for client-server communication in Vite plugins",
  "keywords": [],
  "homepage": "https://github.com/antfu/vite-dev-rpc#readme",
  "bugs": {
    "url": "https://github.com/antfu/vite-dev-rpc/issues"
  },
  "license": "MIT",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/vite-dev-rpc.git"
  },
  "funding": "https://github.com/sponsors/antfu",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "play": "nr dev && vite playground",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vite": "^2.9.0-beta.9"
  },
  "dependencies": {
    "birpc": "^0.1.1",
    "vite-hot-client": "^0.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.9",
    "@antfu/ni": "^0.13.2",
    "@babel/types": "^7.17.0",
    "@types/node": "^17.0.23",
    "bumpp": "^7.1.1",
    "eslint": "^8.12.0",
    "esno": "^0.14.1",
    "pnpm": "^6.32.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.2",
    "vite": "^2.9.0-beta.9",
    "vitest": "^0.7.11"
  }
}
